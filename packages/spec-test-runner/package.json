{
  "private": true,
  "name": "@chainsafe/spec-test-runner",
  "description": "Run all spec tests",
  "license": "LGPL-3.0",
  "author": "ChainSafe Systems",
  "homepage": "https://github.com/ChainSafe/lodestar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:ChainSafe/lodestar.git"
  },
  "bugs": {
    "url": "https://github.com/ChainSafe/lodestar/issues"
  },
  "version": "0.1.0",
  "main": "lib/index.js",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map"
  ],
  "scripts": {
    "test:ssz": "mocha --colors 'test/spec/ssz/**/*.test.ts'",
    "test:ssz-main": "mocha --colors 'test/spec/ssz/**/*mainnet.test.ts'",
    "test:ssz-min": "mocha --colors 'test/spec/ssz/**/*minimal.test.ts'",
    "test:spec": "mocha --colors --exclude 'test/spec/ssz/**/*' 'test/spec/**/*.test.ts' && yarn test:ssz",
    "test:spec-main": "mocha --colors --exclude 'test/spec/ssz/**/*' 'test/spec/**/*mainnet.test.ts' && yarn test:ssz-main",
    "test:spec-min": "mocha --colors --exclude 'test/spec/ssz/**/*' 'test/spec/**/*minimal.test.ts' 'test/spec/bls/*.test.ts' && yarn test:ssz-min"
  },
  "dependencies": {
    "@chainsafe/bit-utils": "0.1.6",
    "@chainsafe/bls": "0.2.1",
    "@chainsafe/eth2.0-config": "0.2.0",
    "@chainsafe/eth2.0-params": "0.2.0",
    "@chainsafe/eth2.0-spec-test-util": "0.4.2",
    "@chainsafe/eth2.0-state-transition": "0.2.0",
    "@chainsafe/eth2.0-types": "0.2.0",
    "@chainsafe/eth2.0-utils": "0.1.0",
    "@chainsafe/lodestar-validator": "0.2.0",
    "@chainsafe/lodestar": "0.2.0",
    "@chainsafe/ssz": "0.5.2",
    "@types/chai": "4.2.0",
    "@types/chai-as-promised": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/yargs": "^13.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.2.0"
  },
  "devDependencies": {
  },
  "keywords": [
    "Ethereum",
    "Eth2",
    "spec",
    "tests"
  ]
}
